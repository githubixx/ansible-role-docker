#jinja2: trim_blocks:False
[Unit]
Documentation=https://github.com/docker/docker/tree/master/contrib/init/systemd
Description=Docker Socket for the API
PartOf=docker.service

[Socket]
ListenStream=/run/docker.sock
SocketMode=0660
SocketUser=root
SocketGroup={{docker_group}}

[Install]
WantedBy=sockets.target
